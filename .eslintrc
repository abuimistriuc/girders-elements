{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "plugins": [
    "react",
    "redux-saga"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "rules": {
    "react/display-name": [0]
  },
  "globals": {
    "Promise": true,
    "Symbol": true
  }
}
